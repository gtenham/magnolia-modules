<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  	<name>magno-resources-amd</name>
  	<displayName>${project.name}</displayName>
  	<description>${project.description}</description>
  	<class>nl.magnodom.resources.ResourcesModule</class>
  	<versionHandler>nl.magnodom.resources.setup.ResourcesModuleVersionHandler</versionHandler>
  	<version>${project.version}</version>

	<components>
		<id>app-resources-amd</id>
	</components>

	<components>
		<id>app-resources-amd-browser</id>
	</components>

	<components>
		<id>app-resources-amd-detail</id>
	</components>
	
  	<dependencies>
    	<dependency>
      		<name>core</name>
      		<version>5/*</version>
    	</dependency>
    	<dependency>
			<name>ui-admincentral</name>
			<version>5/*</version>
		</dependency>
    	<dependency>
      		<name>resources</name>
      		<version>2/*</version>
    	</dependency>
  	</dependencies>
